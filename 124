local Lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/TimkaZXC/ASXZQWERDCVBNNHGFDERFGBNHYUIKM-LQWRQUIGGDYQF-WF-DQR-RS-A-DRUYAWGG-RS/refs/heads/main/Library_V2"))()
local Hub = Lib.Hub("ğŸ€[ğŸ„]Undead Defense")
local Kill = Hub:Section("[ğŸ„]Main", Color3.fromRGB(255,0,0))
Kill:Title("[ğŸ„]Kill Aura's", Color3.fromRGB(255,0,0))
Kill:Toggle("[ğŸ§Ÿâ€â™‚ï¸]Zombie", Color3.new(1,1,1), function(s)
local function Test()
pcall(function()
for _, plr in pairs(workspace.Living:GetChildren()) do
if plr:IsA("Model") then

local player = game:GetService("Players").LocalPlayer
local distance = (player.Character.HumanoidRootPart.Position - plr:FindFirstChildOfClass("Part").Position or plr:FindFirstChildOfClass("MeshPart").Position).Magnitude
if distance < 15 then
if plr.Name ~= player.Name then

local args = {
    [1] = {
        ["Instance"] = plr:FindFirstChildOfClass("Part") or plr:FindFirstChildOfClass("MeshPart"),
        ["Material"] = Enum.Material.Neon,
        ["Normal"] = Vector3.new(0, 999, 0),
        ["Position"] = plr:FindFirstChildOfClass("Part").Position or plr:FindFirstChildOfClass("MeshPart").Position
    },
    [2] = Vector3.new(0, 999, 0)
}

game:GetService("ReplicatedStorage").Remotes.GunSystem.SwingMelee:FireServer(unpack(args))
end
end

end
end

end)
end

if s then
Test()
else

end
end)

