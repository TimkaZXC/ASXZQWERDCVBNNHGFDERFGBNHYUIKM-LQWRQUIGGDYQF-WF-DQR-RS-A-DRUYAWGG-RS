Auto:Toggle("Buy Buttons", Color3.new(1,1,1), function(s)
local function Test()
local player = game:GetService("Players").LocalPlayer

for _, findH in pairs(workspace.Hotels:GetChildren()) do
for n, v in pairs(findH:GetAttributes()) do

if n == player.Name then
local OwnHotel = findH

for _, button in pairs(OwnHotel.Buttons:GetChildren()) do
local args = {
    [1] = OwnHotel,
    [2] = button
}

game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.7.0").knit.Services.TycoonService.RF.BuyButton:InvokeServer(unpack(args))
end

end

end
end

end

if s then
Test()
else

end
end)


